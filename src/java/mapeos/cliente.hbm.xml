<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class name="modelo.cliente" table="cliente">
  <id name="Id_Cliente" column="id_cliente">
          <generator class="identity"/>
      </id>
      
      <property name="Nombre" column="nombre" type="string"/>
      <property name="Apellido" column="apellido" type="string"/>
      <property name="Telefono" column="telefono" type="string" />
      <property name="Direccion" column="direccion" type="string" />
      <property name="Sexo" column="sexo" type="char" />
      
      
  </class>
</hibernate-mapping>
