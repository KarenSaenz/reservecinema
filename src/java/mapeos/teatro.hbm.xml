<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="modelo.teatro" table="teatro">
  <id name="Id_Teatro" column="id_teatro">
          <generator class="identity"/>
      </id>
      
      <property name="Nombre" column="nombre" type="string"/>
      <property name="Direccion" column="direccion" type="string" />
      <property name="Ciudad" column="ciudad" type="string" />
      <property name="Telefono" column="telefono" type="string" />
      <property name="correo" column="correo" type="string" />
      
      
  </class>
</hibernate-mapping>
